<?xml version="1.0" encoding="utf-8"?>
<vectordb>
    <ivfflat>
        <name>IVF_FLAT</name>
        <desc>Inverted File Flat Index</desc>
        <session_params>
            <ss_probes>10</ss_probes>
        </session_params>
        <search_params>
            <se_k>10</se_k>
            <se_distance>cosine</se_distance>
        </search_params>
        <index_params>
            <in_max_parallel_workers>4</in_max_parallel_workers>
            <in_maintenance_work_mem>8GB</in_maintenance_work_mem>
        </index_params>
        <index_creation_with_options>
            <ino_lists>100</ino_lists>
        </index_creation_with_options>
    </ivfflat>
    <hnsw>
        <name>HNSW</name>
        <desc>Hierarchical Navigable Small World</desc>
        <session_params>
            <ss_hnsw.ef_search>100</ss_hnsw.ef_search>
        </session_params>
        <search_params>
            <se_k>10</se_k>
            <se_distance>cosine</se_distance>
        </search_params>
        <index_params>
            <in_max_parallel_workers>4</in_max_parallel_workers>
            <in_maintenance_work_mem>8GB</in_maintenance_work_mem>
        </index_params>
        <index_creation_with_options>
            <ino_ef_construction>200</ino_ef_construction>
            <ino_m>16</ino_m>
        </index_creation_with_options>
    </hnsw>
    <hnsw_bq>
        <name>HNSW - BQ</name>
        <desc>Hierarchical Navigable Small World (Binary Quantization)</desc>
        <session_params>
            <ss_hnsw.ef_search>100</ss_hnsw.ef_search>
        </session_params>
        <search_params>
            <se_k>10</se_k>
            <se_distance>cosine</se_distance>
            <bq_rerank_distance>hamming</bq_rerank_distance>
            <bq_quantized_fetch_limit>100</bq_quantized_fetch_limit>
            <bq_dim>1536</bq_dim>
            <bq_reranking>true</bq_reranking>
        </search_params>
        <index_params>
            <in_max_parallel_workers>4</in_max_parallel_workers>
            <in_maintenance_work_mem>8GB</in_maintenance_work_mem>
        </index_params>
        <index_creation_with_options>
            <ino_ef_construction>200</ino_ef_construction>
            <ino_m>16</ino_m>
        </index_creation_with_options>
    </hnsw_bq>
    <pgdiskann>
        <name>PGDiskAnn</name>
        <desc>DiskAnn</desc>
        <session_params>
            <ss_diskann.l_value_is>100</ss_diskann.l_value_is>
        </session_params>
        <search_params>
            <se_k>10</se_k>
            <se_distance>cosine</se_distance>
        </search_params>
        <index_params>
            <in_max_parallel_workers>4</in_max_parallel_workers>
            <in_maintenance_work_mem>8GB</in_maintenance_work_mem>
        </index_params>
        <index_creation_with_options>
            <ino_max_neighbors>128</ino_max_neighbors>
            <ino_l_value_ib>16</ino_l_value_ib>
        </index_creation_with_options>
    </pgdiskann>
    <mixed_workload>
        <mw_vector_vu>1</mw_vector_vu>
        <mw_oltp_vu>1</mw_oltp_vu>
        <vector_table_name>public.pg_vector_collection</vector_table_name>
    </mixed_workload>
</vectordb>